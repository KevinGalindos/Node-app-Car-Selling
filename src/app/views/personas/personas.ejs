<html lang="es">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
    integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

  <title>PERSONAS</title>
</head>

<body>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark mx-auto">
    <a class="navbar-brand mx-auto" href="#">PERSONAS</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
      aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav mx-auto">
        <li class="nav-item active">
          <a class="nav-link mx-auto" href="#">INICIO <span class="sr-only">(current)</span></a>
        </li>
        <li class="nav-item mx-auto">
          <a class="nav-link mx-auto" href="#">VEHICULOS</a>
        </li>
        <li class="nav-item mx-auto">
          <a class="nav-link mx-auto" href="#">CLIENTES</a>
        </li>

      </ul>
    </div>
  </nav>


  <div class="row">
    <div class="col-md-2 mt-2 text-center">
      <table class="table">

        <thead class="thead-dark">

          <tr>

            <th>ID</th>
            <th>cedula</th>
            <th>Tipo persona</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Correo</th>

          </tr>

        </thead>

        <% for(var i=0; i < automovil.length; i++){ %>

        <tr>

          <td><%= automovil[i].idPersona%></td>
          <td><%= automovil[i].cedula%></td>
          <td><%= automovil[i].tipoPer%></td>
          <td><%= automovil[i].nombre%></td>
          <td><%= automovil[i].apellido %></td>
          <td><%= automovil[i].correo %></td>


        </tr>

        <% } %>
      </table>

    </div>

  </div>


</body>

</html>